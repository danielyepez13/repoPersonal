{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "../generated/prisma/client/**/*",
        "outDir": "./dist/generated/prisma/client"
      }
    ],
    "watchAssets": true
  }
}
